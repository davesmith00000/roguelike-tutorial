{
  "scripts": {
    "start": "parcel index.html --no-cache --out-dir dist --log-level 4 --no-source-maps",
    "build": "parcel build index.html --out-dir dist --log-level 4 --no-source-maps"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0"
  },
  "dependencies": {
    "target": "^0.0.2"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "target/indigoBuild/assets",
        "staticOutDir": "assets",
        "watcherGlob": "target/indigoBuild/assets/**/*"
      }
    ]
  }
}
